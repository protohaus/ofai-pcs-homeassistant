# Use this file to store secrets like usernames and passwords.
# Learn more at https://www.home-assistant.io/docs/configuration/secrets/

# wifi Access Point Credentials
wifi_ap_ssid: ssid
wifi_ap_pw: super_secret_password

# Node red credentials
node_red_admin_usr: username
node_red_admin_pw: super_secret_password
node_red_credential_secret: different_super_secret_password

# Samba Share credentials
smb_share_usr: username
smb_share_pw: super_secret_password

# MQTT Broker
mqtt_usr: username
mqtt_pw: super_secret_password

# Host configuration
host_name: hostname
host_eth0_static_ip: 192.168.1.1
host_wlan0_static_ip: 192.168.2.1
host_wlan0_netmask: 255.255.255.0
host_wlan0_broadcast: 192.168.2.255
host_wlan0_dhcp_start_addr: 192.168.2.10
host_wlan0_dhcp_end_addr: 192.168.2.99
host_wlan0_client_internet_access: 1

#Device Tracker
device_tracker_hosts_range: 192.168.2.1/24
